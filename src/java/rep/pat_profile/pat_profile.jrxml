	<subDataset name="phones" uuid="edd5eb9f-1afb-4c39-a015-8885e43d1001">
		<parameter name="p_id" class="java.lang.Long">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<queryString>
			<![CDATA[select
    ph.phone_raw
   ,tt.type_name
   ,ph.notes
from patient_phone ph
     ,tel_type tt
where ph.tel_type_id = tt.id(+)
  and ph.patient_id = $P{p_id}]]>
		</queryString>
		<field name="PHONE_RAW" class="java.lang.String"/>
		<field name="TYPE_NAME" class="java.lang.String"/>
		<field name="NOTES" class="java.lang.String"/>
	</subDataset>
	<parameter name="p_id" class="java.lang.Long">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[select
    p.id
   ,p.name
   ,p.first_name
   ,p.second_name
   ,p.pseudo_name
   ,decode(p.sex,'F','Женский','M','Мужской') as sex
   ,p.birth_date
   ,round((trunc(sysdate)-p.birth_date)/365,0) as age
   ,p.adr_country
   ,decode(p.adr_country,'Украина',us.rus_name,p.adr_district) as adr_district
   ,decode(p.adr_country,'Украина',ut.rus_name,p.adr_city) as adr_town 
   ,p.adr_street||' '||p.adr_house||decode(trim(p.adr_flat),null,'',', '||p.adr_flat) as addr
   ,p.cur_height
   ,p.cur_weight
   ,p.pass_ser
   ,p.pass_no
   ,p.pass_date
   ,p.pass_gived_by
   ,decode(p.adr_country,'Украина',ut_wk.rus_name,p.work_town) as work_town
   ,p.work_place
   ,decode(p.is_foto_ads_allowed,'T','Да','Нет') as is_foto_ads_allowed
   ,decode(p.is_tv_ads_allowed,'T','Да','Нет') as is_tv_ads_allowed
   ,decode(p.is_other_ads_allowed,'T','Да','Нет') as is_other_ads_allowed
   ,decode(p.is_checked,'T','Да','Нет') as is_checked
   ,decode(p.is_interest,'T','Да','Нет') as is_interest
   ,p.create_date
   ,p.last_psg_date
   ,p.self_story
   ,p.nurse_notes
   ,p.special_notes
   ,p.doc_conlusion
   ,p.cured_by_doc
   ,decode(p.is_lor_invasion,'T','Да','Нет') as is_lor_invasion
   ,p.lor_invasion_details
   ,p.PATALOGY_DETAIL
   ,decode(p.is_allergic,'T','Да','Нет') as is_allergic
   ,p.allergic_detail
   ,decode(p.is_gepatit,'T','Да','Нет') as is_gepatit
   ,decode(p.is_botkin,'T','Да','Нет') as is_botkin
   ,p.next_psg_date
   ,p.partner_name
   ,p_phone.phone_raw as partner_phone 
   ,p.fam_member_name
   ,p_fammbr.phone_raw as mbr_phone
   ,p.email_addr_list
   ,p.cpap_status
   ,man_usr.short_name as managed_by
   ,cre_usr.short_name as created_by
   ,p.ref_info_src_detail
   ,ri.name as ref_info_type
   ,sr.name as send_rank
   ,vr.name as vip_rank
   ,trim(ref_pat.name||' '||ref_pat.first_name||' '||ref_pat.second_name||' '||ref_pat.pseudo_name) as ref_pat_name
   ,p.ref_info_patient_id
   ,de.fio as ref_doc_fio
   ,p.ref_info_doc_id
   ,ph.file_data as ava_file
   
from patient p 
    ,ua_state us
    ,ua_town ut
    ,ua_town ut_wk
    ,patient_photo ph
    ,cardio_user cre_usr
    ,cardio_user man_usr
    ,vip_rank vr
    ,send_rank sr
    ,doctors_external de
    ,patient_phone p_phone
    ,patient_phone p_fammbr
    ,ref_info_source_type ri
    ,patient ref_pat
 where
       p.adr_ua_town_id = ut.id(+)
   and p.adr_ua_state_id = us.id(+)
   and p.work_ua_town_id = ut_wk.id(+)
   and p.avatar_id = ph.id(+)
   and p.created_by_user_id = cre_usr.id(+)
   and p.tracked_by_user_id = man_usr.id(+)
   and p.vip_rank_id = vr.id(+)
   and p.ref_rank_id = sr.id(+)
   and p.ref_info_doc_id = de.id(+)
   and p.partner_phone_id = p_phone.id(+)
   and p.fam_member_phone_id = p_fammbr.id(+)
   and p.ref_info_src_type_id = ri.id(+)
   and p.ref_info_patient_id = ref_pat.id(+)
   and p.id = $P{p_id}]]>
	</queryString>
